  scale_fill_manual(values=c("gray", "royalblue"),#
                    name="",#
                    breaks=c("Gene_Ratio", "Novel_Ratio"),#
                    labels=c("Novel Genes", "SynSig"))+#
  theme_ipsum() +#
  theme_bw()+#
#
  theme(panel.border = element_rect(colour = "black", fill=NA), panel.grid.major = element_blank(), panel.grid.minor = element_blank())+#
  theme(panel.background=element_blank())#
