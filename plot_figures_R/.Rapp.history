df.m<-melt(df, id.vars="Features", measure.vars=c("Training_Ratio", "Exp_Ratio"))#
#
# prepare data for plotting#
plotting_df <-#
  df.m %>% #
  # a trick!#
  mutate(value = if_else(variable == "Training_Ratio", -value, value))#
#
## find the order#
temp_df <-#
  plotting_df %>% #
  filter(variable == "Exp_Ratio") %>% #
  arrange(value)#
the_order <- temp_df$Features#
# plot#
p <- #
  plotting_df %>% #
  ggplot(aes(x = Features, y = value, group = variable, fill = variable)) +#
  geom_bar(stat = "identity", width = 0.75) +#
  coord_flip() +#
  scale_x_discrete(limits = the_order) +#
  # another trick!#
  scale_y_continuous(breaks = seq(-3, 3, 1), labels = abs(seq(-3, 3, 1))) +#
  #labs(x = "Features", y = "Fold Change", title = "Genomic and Protein Features") +#
  labs(x = "Features", y = "Fold Change") +#
  theme(legend.position = "bottom",#
        legend.title = element_blank(),#
        plot.title = element_text(hjust = 0.5),#
        panel.background = element_rect(fill =  "grey90")) +#
