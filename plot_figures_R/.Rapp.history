library(dplyr)#
library(tidyr)#
library(ggplot2)#
library(ggthemes)#
#
df <- read.csv('/Users/karenmei/Documents/Synapse_Paper_Code/synapse_11/nonbrain_big_genes_pool_pipeline/synapse_phenotype_heatmap.csv')#
#
df.tidy<- df %>% tidyr::gather(sample, phenotypes, 2:27)#
#
df.tidy$phenotypes<- factor(df.tidy$phenotypes)#
#
## use a white border of size 0.5 unit to separate the tiles#
gg<- ggplot(df.tidy, aes(x=sample, y=X, fill=phenotypes)) + geom_tile(color="white", size=0.5)#
#
gg<- gg + scale_fill_brewer(palette = "Set1", direction = -1)#
gg<- gg + coord_equal()#
## add title#
#
gg<- gg + labs(x=NULL, y=NULL)#
#
library(ggthemes)#
##starting with a base theme of theme_tufte() from the ggthemes package. It removes alot of chart junk without having to do it manually.#
gg <- gg + theme_tufte(base_family="Helvetica")#
#
#We don’t want any tick marks on the axes and I want the text to be slightly smaller than the default.#
#
gg <- gg + theme(axis.ticks=element_blank())#
gg <- gg + theme(axis.text.x=element_text(angle = 45, hjust = 1))#
gg#
#
gg <- ggplot(df.tidy, aes(x=sample, y=X, fill=phenotypes)) + geom_tile(color="white", size=0.5) +#
         coord_equal() +#
        labs(x=NULL, y=NULL) +#
        theme_tufte(base_family="Helvetica") +#
        scale_fill_manual(name= "Phenotypes", labels= c("Absent", 'Present'), values = c("gray", "red")) +#
        theme(axis.ticks=element_blank()) + #
        theme(axis.text.x=element_text(angle = 45, hjust = 1))+#
        theme(text = element_text(size=25))
