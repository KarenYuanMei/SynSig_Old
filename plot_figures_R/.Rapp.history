df <- read.csv("/Users/karenmei/Documents/Synapse_Paper_Code/synapse_11/update_enrichment/SynSig_enrichment_R.csv")#
#
df
df.m<-melt(df, id.vars="Functions", measure.vars=c("Gene_Ratio", "Novel_Ratio"))#
#
# prepare data for plotting#
plotting_df <-#
  df.m %>% #
  # a trick!#
  mutate(value = if_else(variable == "Gene_Ratio", -value, value))#
