df <- read.csv("/Users/karenmei/Documents/Synapse_Paper_Code/synapse_11/nonbrain_big_genes_pool_pipeline/synapse_syndromic_phe_enrichment.csv")#
#
df.m<-melt(df, id.vars="X", measure.vars=c("Synapse_Fold", "Syndromic_Fold"))#
#
# prepare data for plotting#
plotting_df <-#
  df.m %>% #
  # a trick!#
  mutate(value = if_else(variable == "Synapse_Fold", -value, value))#
#
## find the order#
temp_df <-#
  plotting_df %>% #
  filter(variable == "Syndromic_Fold") %>% #
  arrange(value)#
the_order <- temp_df$X#
# plot#
p <- #
  plotting_df %>% #
  ggplot(aes(x = X, y = value, group = variable, fill = variable)) +#
  geom_bar(stat = "identity", width = 0.75) +#
  coord_flip() +#
  scale_x_discrete(limits = the_order) +#
  # another trick!#
  scale_y_continuous(breaks = seq(-2, 2, 1), labels = abs(seq(-2, 2, 1))) +#
  #labs(x = "Features", y = "Fold Change", title = "Genomic and Protein Features") +#
  labs(x = "Phenotypes", y = "Fold Enrichment") +#
  theme(legend.position = "bottom",#
        legend.title = element_blank(),#
        plot.title = element_text(hjust = 0.5),#
        panel.background = element_rect(fill =  "grey90"))
p
p+scale_fill_manual(values=c("pink", "red3")
p
p+scale_fill_manual(values=c("pink", "red3"))
p
p+scale_fill_manual(values=c("pink", "red3"))+#
  theme_ipsum() +#
  theme_bw()
